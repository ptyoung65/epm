{
  "name": "airis-mon-korean-dashboard",
  "version": "1.0.0",
  "description": "Korean-style HyperDX dashboard for AIRIS-MON with ClickStack integration",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "dev": "npm run build:css && npm start"
  },
  "keywords": [
    "dashboard",
    "hyperdx",
    "korean",
    "observability",
    "clickstack",
    "monitoring",
    "airis-mon",
    "react"
  ],
  "author": "AIRIS-MON Team",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/x-charts": "^6.0.0-alpha.0",
    "@playwright/test": "^1.55.0",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.2.4",
    "lodash": "^4.17.21",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^4.3.8",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "recharts": "^2.5.0",
    "socket.io-client": "^4.6.1",
    "styled-components": "^5.3.9",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.4",
    "tailwindcss": "^4.1.12",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3002",
  "homepage": "http://./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}
