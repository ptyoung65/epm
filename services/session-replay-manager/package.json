{
  "name": "@airis-epm/session-replay-manager",
  "version": "1.0.0",
  "description": "AIRIS EPM OpenReplay 통합 관리자 서비스 - 세션 리플레이 및 관리자 모니터링",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp -r src/public dist/ 2>/dev/null || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -f Dockerfile.admin -t airis-epm/session-replay-manager .",
    "docker:run": "docker run -p 4000:4000 airis-epm/session-replay-manager",
    "openreplay:up": "docker-compose -f docker-compose.openreplay.yml --env-file .env.openreplay up -d",
    "openreplay:down": "docker-compose -f docker-compose.openreplay.yml down",
    "openreplay:logs": "docker-compose -f docker-compose.openreplay.yml logs -f",
    "openreplay:restart": "docker-compose -f docker-compose.openreplay.yml restart",
    "setup": "npm install && npm run build",
    "clean": "rm -rf dist node_modules coverage",
    "migration:run": "node dist/migrations/index.js",
    "seed": "node dist/seeds/index.js"
  },
  "keywords": [
    "session-replay",
    "openreplay",
    "monitoring",
    "analytics",
    "user-experience",
    "airis-epm",
    "admin-dashboard",
    "real-time"
  ],
  "author": "AIRIS EPM Development Team <dev-team@airis-epm.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.18.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.1.0",
    "pg": "^8.12.0",
    "pg-pool": "^3.6.2",
    "redis": "^4.6.8",
    "ioredis": "^5.4.1",
    "axios": "^1.7.2",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.4",
    "ws": "^8.17.1",
    "socket.io": "^4.7.5",
    "nodemailer": "^6.9.13",
    "handlebars": "^4.7.8",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "uuid": "^10.0.0",
    "mime-types": "^2.1.35",
    "archiver": "^7.0.1",
    "csv-parser": "^3.0.0",
    "json2csv": "^5.0.7",
    "mongodb": "^6.8.0",
    "@clickhouse/client": "^0.2.10"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express-session": "^1.18.0",
    "@types/pg": "^8.11.6",
    "@types/multer": "^1.4.11",
    "@types/ws": "^8.5.10",
    "@types/nodemailer": "^6.4.15",
    "@types/lodash": "^4.17.5",
    "@types/uuid": "^10.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/archiver": "^6.0.2",
    "@types/mongodb": "^4.0.7",
    "typescript": "^5.5.2",
    "tsx": "^4.15.7",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.5",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "nodemon": "^3.1.4",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airis-epm/session-replay-manager"
  },
  "bugs": {
    "url": "https://github.com/airis-epm/session-replay-manager/issues"
  },
  "homepage": "https://github.com/airis-epm/session-replay-manager#readme"
}