{
  "name": "@airis-epm/compatibility-layer",
  "version": "1.0.0",
  "description": "AIRIS APM to EPM Compatibility Layer - Seamless API Transition",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:compatibility": "jest --testPathPattern=compatibility",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t localhost:5000/airis-epm-compatibility:latest .",
    "docker:push": "docker push localhost:5000/airis-epm-compatibility:latest",
    "migrate": "ts-node src/scripts/migrate.ts",
    "validate-apis": "ts-node src/scripts/validate-apis.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "clickhouse": "^2.6.0",
    "mongodb": "^6.2.0",
    "xml2js": "^0.6.2",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "openapi-types": "^12.1.3",
    "jsonpath": "^1.1.1",
    "fast-xml-parser": "^4.3.2",
    "prometheus-api-metrics": "^3.2.2",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.6",
    "@types/multer": "^1.4.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/pg": "^8.10.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "openapi-generator": "^1.6.10"
  },
  "engines": {
    "node": ">=18.0.0",
