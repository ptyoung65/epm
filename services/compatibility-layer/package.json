  },
  "repository": {
    "type": "git",
    "url": "https://github.com/company/airis-epm.git"
  },
  "keywords": [
    "compatibility",
    "adapter",
    "api-migration",
    "rest-api",
    "graphql",
    "airis",
    "apm",
    "epm",
