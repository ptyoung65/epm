{
  "name": "airis-apm",
  "version": "1.0.0",
  "description": "AIRIS Application Performance Monitoring System - Java-based APM with Real-time Monitoring and Performance Analysis",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node scripts/start.js",
    "start:core": "node src/index.js",
    "demo": "node examples/demo.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:validation": "ts-node tests/validation/run-validation-suite.ts",
    "test:validation:production": "ts-node tests/validation/run-validation-suite.ts --no-performance --no-integration",
    "test:validation:full": "ts-node tests/validation/run-validation-suite.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t airis-mon .",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "apm:start": "NODE_ENV=production node scripts/start.js",
    "apm:dev": "NODE_ENV=development nodemon scripts/start.js",
    "apm:agent": "node src/apm/agent.js",
    "apm:profiler": "node src/apm/profiler.js"
  },
  "keywords": [
    "apm",
    "application-performance-monitoring",
    "java-monitoring",
    "j2ee",
    "performance-analysis",
    "real-time-monitoring",
    "byte-code-instrumentation",
    "jvm-profiling",
    "jdbc-monitoring",
    "system-monitoring"
  ],
  "author": "AIRIS-APM Team",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^0.2.5",
    "@influxdata/influxdb-client": "^1.33.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.0.1",
    "fastify": "^4.24.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "rate-limiter-flexible": "^2.4.2",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.21",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "java": ">=11.0.0"
  },
  "apm": {
    "version": "1.0.0",
    "features": [
      "real-time-monitoring",
      "j2ee-application-monitoring",
      "jdbc-monitoring",
      "jvm-profiling",
      "byte-code-instrumentation",
      "performance-analysis",
      "statistical-reporting",
      "dashboard-views"
    ],
    "requirements": {
      "java": ">=11.0.0",
      "jvm": "HotSpot or OpenJ9",
      "memory": ">=2GB",
      "storage": ">=10GB"
    }
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  }
}
